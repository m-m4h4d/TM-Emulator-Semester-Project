{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/tm_core.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/tm_core",
      "dest": "api/tm_core.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}